{% for python_version in python_versions %}
- name: Dump YAML
  preconditions:
    python_version: {{ python_version }}
  tags:
    - dump
    - py{{ python_version }}
  scenario:
    - Assert true: >
        dumbyaml.dump({'a': 3.5, 'b': False, 'c': 'text'}) == 'a: 3.5\nb: false\nc: text\n'
{% endfor %}
